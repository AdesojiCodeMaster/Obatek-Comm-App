<!DOCTYPE html>
<html>
  <head>
    <title>ObatekCommApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    </head>

  <body id= 'body'>

 <p class="notice"><%= notice %></p>
       <p class="alert"><%= alert %></p>
    <%= yield %>
    <% if current_customer %>  
      <button class='front-log'><%= link_to " Logout", destroy_customer_session_path, :method => :delete, class: "destroy_member_session" %></button>
    <%else%>
      <button class='front-log' ><%= link_to "Sign up", new_customer_registration_path,  class: "new__customer_registration" %></buttton>
      <button class='front-log'><%= link_to "Login", new_customer_session_path,  class: "new_customer_session" %></button>
  <% end %>  
  </body>
</html>
